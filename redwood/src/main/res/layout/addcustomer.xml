<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5">

    <include
        layout="@layout/addcustomer_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        app:layout_heightPercent="13%w"
        app:layout_widthPercent="100%w" />

    <include
        android:id="@+id/in_id_new"
        layout="@layout/addcustomer_new"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        app:layout_marginTopPercent="16%w"
        app:layout_widthPercent="100%w" />

    <include
        android:id="@+id/in_id_create"
        layout="@layout/addcustomer_phonelist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/in_id_new"
        app:layout_widthPercent="100%w" />
</com.zhy.android.percent.support.PercentRelativeLayout>