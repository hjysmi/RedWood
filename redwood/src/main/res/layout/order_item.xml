<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/rl_main"
    android:background="@color/f5f5f5">
    <View
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_heightPercent="1.2%w"
        android:layout_alignParentTop="true"/>
    <include
        android:layout_below="@+id/top"
        android:id="@+id/in_order_item_top"
        layout="@layout/order_item_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_heightPercent="10%w" />
    <View
        android:id="@+id/line"
        android:layout_below="@+id/in_order_item_top"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/eeeeee"/>
    <include
        android:id="@+id/order_pro_item"
        android:layout_below="@+id/line"
        layout="@layout/order_pro_item"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        />

    <include
        android:id="@+id/in_order_item_bottom"
        layout="@layout/order_item_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/order_pro_item"
        app:layout_heightPercent="10%w" />


</com.zhy.android.percent.support.PercentRelativeLayout>