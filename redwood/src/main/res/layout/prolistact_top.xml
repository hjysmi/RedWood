<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/prolistact_bg">

    <ImageView
        android:id="@+id/id_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/back_prolist"
        app:layout_heightPercent="12.5%w"
        app:layout_widthPercent="12.5%w" />

    <include
        android:id="@+id/in_prolistact_top"
        layout="@layout/prolistact_et"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        app:layout_heightPercent="12%w"
        app:layout_widthPercent="75%w" />

    <include
        layout="@layout/prolistact_center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/in_prolistact_top"
        app:layout_heightPercent="10%w" />

</com.zhy.android.percent.support.PercentRelativeLayout>